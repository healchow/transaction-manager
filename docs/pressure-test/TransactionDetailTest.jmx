          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell 后置处理程序" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.util.ArrayList;

// 获取之前保存的结果列表，如果不存在则创建一个新的列表
ArrayList&lt;String&gt; resultList = vars.getObject(&quot;added_tid_list&quot;);

if (resultList == null) {
    resultList = new ArrayList&lt;&gt;();
}

// 获取本次提取的结果
String result = vars.get(&quot;added_tid&quot;);
if (result != null) {
    resultList.add(result);
}

// 将更新后的列表保存回变量
vars.putObject(&quot;added_tid_list&quot;, resultList);</stringProp>
          </BeanShellPostProcessor>
