          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell 预处理程序" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import java.util.Random;

Random random = new Random();

// 常见姓氏和名字数组
String[] surnames = {&quot;赵&quot;, &quot;钱&quot;, &quot;孙&quot;, &quot;李&quot;, &quot;周&quot;, &quot;吴&quot;, &quot;郑&quot;, &quot;王&quot;, &quot;冯&quot;, &quot;陈&quot;, &quot;褚&quot;, &quot;卫&quot;, &quot;蒋&quot;, &quot;沈&quot;, &quot;韩&quot;, &quot;杨&quot;};
String[] givenNames = {&quot;伟&quot;, &quot;芳&quot;, &quot;娜&quot;, &quot;敏&quot;, &quot;静&quot;, &quot;强&quot;, &quot;磊&quot;, &quot;军&quot;, &quot;洋&quot;, &quot;勇&quot;, &quot;艳&quot;, &quot;杰&quot;, &quot;娟&quot;, &quot;涛&quot;, &quot;明&quot;, &quot;超&quot;};

// 随机组合生成姓名
String fullName = surnames[random.nextInt(surnames.length)] + givenNames[random.nextInt(givenNames.length)];
vars.put(&quot;random_name&quot;, fullName);

// 随机生成 0 - 99 的整数
int randomNumber = random.nextInt(100);
vars.put(&quot;random_num&quot;, String.valueOf(randomNumber));

// 随机选择交易操作
String[] types = {&quot;DEPOSIT&quot;, &quot;WITHDRAWAL&quot;, &quot;TRANSFER_OUT&quot;};
String type = types[random.nextInt(types.length)];
vars.put(&quot;random_type&quot;, type);
</stringProp>
          </BeanShellPreProcessor>
